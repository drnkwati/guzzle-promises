{
    "name": "drnkwati/guzzle-promises",
    "description": "PHP 5.3-compatible fork of guzzle/promises.",
    "keywords": ["promise"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dowling",
            "email": "mtdowling@gmail.com",
            "homepage": "https://github.com/mtdowling"
        },
        {
            "name": "Eric Hough",
            "email": "eric@tubepress.com",
            "homepage": "https://github.com/ehough"
        },
        {
            "name": "Nkwati Divine",
            "email": "drnkwati@gmail.com",
            "homepage": "http://dnkwati.org"
        }
    ],
    "require": {
        "php": ">=5.3.29",
        "ehough/generators" : "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\Promise\\": "src/"
        },
        "files": ["src/functions_include.php"]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
